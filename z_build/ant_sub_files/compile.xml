<project name="compile" basedir=".">
   <property name="com_github_xbn" value="com${fs}github${fs}xbn${fs}"/>
   <property name="dir_src_alit" value="${dir_sandbox_src}${com_github_alit}"/>
   <property name="dir_src_feather" value="${dir_sandbox_src}${cg_alit_feather}"/>
   <property name="dir_src_examples" value="${dir_sandbox_examples}${cgx_feather_examples}"/>
   <property name="dir_src_test" value="${dir_sandbox_test}${cgx_feather_test}"/>
   <property name="dir_xbn_project_src_xbn" value="${dir_xbn_project_src}${com_github_xbn}"/>

   <target name="compileaaw" description="Compile all 'xbn' java code, As A Whole.">
      <javac srcdir="${dir_src_alit}"
         destdir="${build.class.dir}"
         excludes="build/**/*"
         classpathref="project_classpath"/>
   </target>

   <target name="compilehelp" description="See XBN-Java project's 'compilehelp' ant task for important information.">
<echo>See XBN-Java project's 'compilehelp' ant task for important information.</echo>
   </target>
   <target name="compileprecodelet">
      <antcall target="compilefeather"/>
      <antcall target="compilefeather2"/>
      <antcall target="compilexmpl"/>
      <antcall target="compiletest"/>
      <antcall target="jar"/>
   </target>
   <target name="jar">
<echo>${dir_build_download}</echo>
      <jar destfile="${dir_build_download}${project_name_for_dir}_all.jar"
         basedir="${build.class.dir}"
         includes="com${fs}github${fs}xbn${fs}**/*.class"
      />
      <jar destfile="${dir_build_download}${project_name_for_dir}_core.jar"
         basedir="${build.class.dir}"
         includes="com${fs}github${fs}xbn${fs}**/*.class"
         excludes="com${fs}github${fs}xbn${fs}examples${fs}**/*.class,com${fs}github${fs}xbn${fs}test${fs}**/*.class"
      />
   </target>

   <target name="compiletest">
      <javac srcdir="${dir_src_test}" destdir="${build.class.dir}" classpathref="project_classpath" debug="true" debuglevel="lines,vars,source" listfiles="true"
         includes="GapCharConfig_Unit.java"/>
      <javac srcdir="${dir_src_test}" destdir="${build.class.dir}" classpathref="project_classpath" debug="true" debuglevel="lines,vars,source" listfiles="true"
         includes="TemplatePiece_Unit.java"/>
