     -->
   <property name="xbn_project_jar_in_build_pre_core_dot_jar" value="${base.dir}\\build\\${xbnjava_project_name_and_version}\\download\\${xbnjava_project_name_and_version}"/>

   <target name="-deleteallxbnclassesandjars">
      <echo>About to delete classes from XBN-Java CLASSES directory (except bootstraps), *and* the xbn-jar from the dependencies directory (they are restored by 'unzipallxbntoclassescopycodeletjarstodependencydir'). Will then compile XBN-Java code **directly from the XBN-Java project sandbox**, but only exactly those classes that are needed to *use* Template Featherweight.</echo>
      <input message="Enter to proceed, Ctrl+C to not."/>
      <delete includeemptydirs="true">
         <fileset dir="${build.class.dir}${fs}${com_github_xbn}" excludes="testdev/ant/*"/>
      </delete>
   </target>

   <target name="unzipallxbntoclassescopycodeletjarstodependencydir">
      <unzip src="${xbn_project_jar_in_build_pre_core_dot_jar}-all.jar" dest="${build.class.dir}"/>
   </target>

   <target name="-compilexbnneededonly" if="compile.xbn.needed.only">
      <antcall target="compilexbnneededonly"/>
   </target>
   <target name="-unzipallxbntoclassescopycodeletjarstodependencydir" if="compile.xbn.needed.only">
      <antcall target="unzipallxbntoclassescopycodeletjarstodependencydir"/>
   </target>
   <target name="compilexbnneededonly" description="Deletes all XBN-Java classes (except bootstraps), compiles and then jars only those needed by Template Feather. All XBN-Java class files are restored by unzipallxbntoclassescopycodeletjarstodependencydir.">

      <antcall target="-compilexbnneededonlyasawhole"/>
      <antcall target="-compilexbnneededonly1by1"/>

      <delete includeemptydirs="true">
         <fileset dir="${dir_sandbox_jar_dependencies}" includes="xbn*.jar"/>
      </delete>

      <property name="xbn_needed_only_jar" value="${dir_sandbox_jar_dependencies}${xbnjava_project_name_and_version}_templatefeather_needed_only.jar"/>

      <jar destfile="${xbn_needed_only_jar}"
         basedir="${build.class.dir}"
         includes="**/xbn/**/*.class"/>

      <copy file="${xbn_needed_only_jar}" overwrite="true"
         todir="${copy_xbn_needed_only_jar_to_dir}"/>
   </target>
   <target name="-compilexbnneededonlyasawhole" if="compile.as.a.whole">
      <antcall target="compilexbnneededonlyasawhole"/>
   </target>
   <target name="-compilexbnneededonly1by1" unless="compile.as.a.whole">
      <antcall target="compilexbnneededonly1by1"/>
      <antcall target="compilexbnneededonly1by1b"/>
   </target>
